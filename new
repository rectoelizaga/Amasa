    <script>
      (function() {
        window.dataLayer = window.dataLayer || [];
        let oneUnitProductPrice = {};
        window._CTR_FingerprintGeneratedCallback = function(fpId) {
          window.dataLayer.push({
            'event': 'Presale',
            'fpid': fpId
          });
          console.log('fired gtm event for Presale');
        };
        //load ctrwow fingerprint analytics script
        try {
          var fingerprint = JSON.parse('{"_CTR_TRACKING_ID":"BS0000361"}');
          window._CTR_TRACKING_ID = fingerprint._CTR_TRACKING_ID;
          if (!!window._CTR_TRACKING_ID) {
            window._CTR_CUSTOM_DATA = {
              siteName: 'beautystatcosmetics',
              pageName: '',
              pageType: 'presale',
              campaignName: '',
              campaignWebKey: ''
            };
            var script = document.createElement('script');
            script.src = '//d16hdrba6dusey.cloudfront.net/ctrwow_fp_analytics.min.js';
            script.defer = true;
            document.querySelector('body').appendChild(script);
          }
        } catch (err) {
          console.log('Bind fingerprint error: ', err);
        }
      })();
    </script><script src="//d16hdrba6dusey.cloudfront.net/ctrwow_fp_analytics.min.js" defer=""></script> <!-- /add gtm event -->

    <!-- Google GTM -->
    <input type="hidden" id="gtmId" value="GTM-M3959XV">
    <script>
      window.addEventListener('load', function() {
        var gtmId = 'GTM-M3959XV';
        var orderGtmId = window.localStorage.getItem('orderGtmId');
        //on confirm page we use a script to enable the variable isUseDifferentGtmId to true
        if (typeof isUseDifferentGtmId !== 'undefined' && isUseDifferentGtmId === true && orderGtmId) {
          gtmId = orderGtmId;
        }
        console.log('init gtm script');
        (function(w, d, s, l, i) {
          w[l] = w[l] || [];
          w[l].push({
            'gtm.start': new Date().getTime(),
            event: 'gtm.js'
          });
          var f = d.getElementsByTagName(s)[0],
            j = d.createElement(s),
            dl = l != 'dataLayer' ? '&l=' + l : '';
          j.async = true;
          j.src =
            'https://www.googletagmanager.com/gtm.js?id=' + i + dl;
          f.parentNode.insertBefore(j, f);
        })(window, document, 'script', 'dataLayer', gtmId);
      });
    </script> <!-- /Google GTM -->

    <!-- add Control CTA event -->
    <script>
      (function() {
        function getUrlParam(param) {
          var href = '';
          if (location.href.indexOf('?') > -1) {
            href = location.href.substr(location.href.indexOf('?'));
          }
          var value = href.match(new RegExp('[\?\&]' + param + '=([^\&]*)(\&?)', 'i'));
          return value ? value[1] : null;
        }
        var pcta = getUrlParam('pcta');
        if (!!pcta && pcta !== '') {
          window.siteSetting.redirectURL = pcta;
        }
      })();
    </script> <!-- /add Control CTA event -->

